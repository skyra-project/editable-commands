{
	"name": "@skyra/editable-commands",
	"version": "2.1.0",
	"description": "A framework agnostic library for editable commands",
	"author": "@skyra",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"homepage": "https://skyra-project.github.io/editable-commands",
	"private": false,
	"files": [
		"dist"
	],
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts",
		"docs": "typedoc",
		"update": "yarn upgrade-interactive --latest",
		"clean": "node scripts/clean-dist.mjs",
		"build": "tsc -b src",
		"postbuild": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"watch": "yarn build -w",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepublishOnly": "yarn clean && yarn build",
		"prepare": "husky install .github/husky"
	},
	"devDependencies": {
		"@commitlint/cli": "^14.1.0",
		"@commitlint/config-conventional": "^14.1.0",
		"@sapphire/eslint-config": "^4.0.4",
		"@sapphire/prettier-config": "^1.2.4",
		"@sapphire/ts-config": "^3.1.4",
		"@types/node": "^16.11.7",
		"@types/ws": "^8.2.0",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"discord-api-types": "^0.24.0",
		"discord.js": "^13.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint": "^8.2.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^11.2.6",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"standard-version": "^9.3.2",
		"typedoc": "^0.22.8",
		"typescript": "^4.4.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/editable-commands.git"
	},
	"engines": {
		"node": ">=16.6",
		"npm": ">=7.24.2"
	},
	"keywords": [
		"discord.js",
		"editable",
		"commands",
		"utility"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/editable-commands/issues"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@sapphire/prettier-config"
}
