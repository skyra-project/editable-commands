{
	"name": "@skyra/editable-commands",
	"version": "2.1.2",
	"description": "A framework agnostic library for editable commands",
	"author": "@skyra",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"homepage": "https://skyra-project.github.io/editable-commands",
	"files": [
		"dist"
	],
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"docs": "typedoc",
		"clean": "node scripts/clean-dist.mjs",
		"build": "tsc -b src && gen-esm-wrapper dist/index.js dist/index.mjs",
		"watch": "tsc -b src -w",
		"sversion": "standard-version",
		"prepublishOnly": "yarn clean && yarn build",
		"prepare": "husky install .github/husky"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.0.2",
		"@commitlint/config-conventional": "^16.0.0",
		"@sapphire/eslint-config": "^4.0.11",
		"@sapphire/prettier-config": "^1.2.9",
		"@sapphire/ts-config": "^3.1.8",
		"@types/node": "^17.0.8",
		"@types/ws": "^8.2.2",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"cz-conventional-changelog": "^3.3.0",
		"discord-api-types": "^0.26.1",
		"discord.js": "^13.6.0",
		"eslint": "~8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.7",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.3.2",
		"typedoc": "^0.22.10",
		"typescript": "^4.5.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/editable-commands.git"
	},
	"engines": {
		"node": ">=16.6",
		"npm": ">=7.24.2"
	},
	"keywords": [
		"discord.js",
		"editable",
		"commands",
		"utility"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/editable-commands/issues"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.1.1"
}
